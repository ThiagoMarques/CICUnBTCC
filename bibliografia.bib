  author={Izquierdo, Luis R. and Izquierdo, Segismundo S. and Sandholm, William H.},
  title={{An introduction to ABED: Agent-based simulation of evolutionary game dynamics}},
  journal={Games and Economic Behavior},
  year=2019,
  volume={118},
  number={C},
  pages={434-462},
  month={},
  keywords={Evolutionary game theory; Agent-based simulations; Population dynamics},
  doi={10.1016/j.geb.2019.09.014},
  abstract={ABED is free and open-source software for simulating evolutionary game dynamics in finite populations. We explain how ABED can be used to simulate a wide range of dynamics considered in the literature and many novel dynamics. In doing so, we introduce a general model of revisions for dynamic evolutionary models, one that decomposes strategy updates into selection of candidate strategies, payoff determination, and choice among candidates. Using examples, we explore ways in which simulations can complement theory in increasing our understanding of strategic interactions in finite populations.},
  url={https://ideas.repec.org/a/eee/gamebe/v118y2019icp434-462.html}
}
